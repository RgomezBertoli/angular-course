{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "globals": {
        "angular": true,
        "jasmine": true,
        "describe": true,
        "it": true,
        "beforeEach": true,
        "expect": true,
        "inject": true
    },
    "rules": {
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-unused-vars": 1,
        "default-case": 2
    }
}